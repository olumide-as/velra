# 游대 Begin Redirect Rules
RewriteEngine On
# 游대 Force HTTPS for all domains
RewriteCond %{HTTPS} off
RewriteRule ^(.*)$ https://%{HTTP_HOST}%{REQUEST_URI} [L,R=301]
# 游대 Redirect www to non-www
RewriteCond %{HTTP_HOST} ^www\.(.+)$ [NC]
RewriteRule ^(.*)$ https://%1/$1 [L,R=301]
# 游대 Redirect all domains to velra.co.uk
RewriteCond %{HTTP_HOST} !^velra\.co\.uk$ [NC]
RewriteRule ^(.*)$ https://velra.co.uk/$1 [L,R=301]
